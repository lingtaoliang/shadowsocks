<?xml version="1.0" encoding="UTF-8"?>
<configuration status="warn">
    <appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy/MM/dd HH:mm:ss.SSS} %t [%p] %c{1} (%F:%L) %msg%n"/>
        </Console>
    </appenders>

    <Loggers>

        <Root level="info">
            <appender-ref ref="Console"/>
        </Root>
    </Loggers>
</configuration>